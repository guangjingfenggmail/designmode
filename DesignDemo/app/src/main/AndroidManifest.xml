<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.open.design">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/plus_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity"
            android:description="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".single.SingletonActivity"
            android:description="@string/app_single"
            />
        <activity android:name=".builder.BuilderActivity"
            android:description="@string/app_builder"
            />
        <activity android:name=".observer.ObserverActivity"
            android:description="@string/app_observer"
            />
        <activity android:name=".ui.PrintActivity"
            android:description="@string/app_name"
            />
        <activity android:name=".ui.WebViewActivity"
            android:description="@string/app_name"
            />
    </application>

</manifest>